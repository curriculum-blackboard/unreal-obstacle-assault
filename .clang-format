BraceWrapping:  
  AfterClass: true  
  AfterControlStatement: true  
  AfterEnum: true  
  AfterFunction: true  
  AfterNamespace: true  
  AfterStruct: true  
  AfterUnion: true  
  AfterExternBlock: true  
  BeforeCatch: true  
  BeforeElse: true  
  IndentBraces: false  
  SplitEmptyFunction: true  
  SplitEmptyRecord: true  
  SplitEmptyNamespace : true  
BreakAfterJavaFieldAnnotations: true  
